{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "downlevelIteration": true,
    "paths": {
      "@/public/*": [
        "public/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/components/utils/*"
      ],
      "@/svgs/*": [
        "src/components/svgs/*"
      ],
      "@/module/*": [
        "src/components/module/*"
      ],
      "@/templates/*": [
        "src/components/templates/*"
      ],
      "@/layout/*": [
        "src/components/layout/*"
      ],
      "@/context/*": [
        "src/components/context/*"
      ],
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/[lang]/education/category/[category]/[subcategory]/page.tsx",
    "src/components/module/sidebar/LoginMenuModule.jsx",
    "src/components/module/sidebar/HeaderMobile.tsx",
    "src/app/[lang]/api/sendEmail/route.ts",
    "src/components/module/levelComponent/TabSelector.tsx",
    ".next/dev/types/**/*.ts"
, "src/app/[lang]/page.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
